{
  "manifest_version": 3,
  "name": "TripleKey Tabs Navigator",
  "version": "1.0",
  "description": "Custom keyboard shortcuts for quick tab navigation and URL access.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "author": "Rohit Wadhwa",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Open the Speed Dial action popup."
    },
    "open-custom-shortcut-0": {
      "suggested_key": {
        "default": "Ctrl+Shift+0",
        "mac": "Command+Shift+0"
      },
      "description": "Open custom shortcut for the first slot"
    },
    "open-custom-shortcut-1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Open custom shortcut for the second slot"
    },
    "open-custom-shortcut-2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "Open custom shortcut for the third slot"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
